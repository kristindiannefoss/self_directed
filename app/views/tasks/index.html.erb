<div class="container">
  <div class="row">
    <div class="task-list">
      <table class="table">
        <tr>
          <th><h2>My Tasks</h2></th>
          <th><h2></h2></th>
          <th><h2></h2></th>
        </tr>
        <% @tasks.each do |task|%>
        <tr>
          <td><p><%= task.description %></p></td>
          <td><p> <%= link_to "Edit", edit_task_path(task), method: :get %> </p></td>
          <td><p> <%= link_to "Delete", delete_task_path(task), method: :delete %> </p></td>
        </tr>
        <% end %>
      </table>
    </div>

  </div>
</div>
